{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1.js" },
    { "src": "^/$", "dest": "/main.html" },
    { "src": "^/main$", "dest": "/main.html" },
    { "src": "^/detail$", "dest": "/detail.html" }
  ]
}
